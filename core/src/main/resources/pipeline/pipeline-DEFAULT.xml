<pipeline name="DEFAULT">

    <processor-chain name="process-form-entity" initial="createOrUpdate">
        <processor-link name="createOrUpdate">
            <processor class="df.base.pipeline.form.FormCreateOrUpdateProcessor" />
            <transition return="CREATE" next="createProcess" />
            <transition return="UPDATE" next="updateProcess" />
        </processor-link>
        <processor-link name="updateProcess">
            <processor class="df.base.pipeline.form.FormUpdateProcessor" />
        </processor-link>
    </processor-chain>

    <!--  Just for test  -->
    <processor-chain name="object-logging-chain">
        <processor-link name="print-object">
            <processor class="df.base.pipeline.common.PrintObjectProcessor" />
        </processor-link>
    </processor-chain>

</pipeline>
